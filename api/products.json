layout: none
permalink: /api/products/
---
[
  {%- for product_hash in site.data.products %}
  {% assign product = product_hash[1] %}
    {
      "title":      {{- product.title | jsonify }},
      "url":        {{- product.url | jsonify }},
      "price":      {{- product.price | jsonify }}
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor -%}
]
